config prog_list
	option enabled '1'
	option program 'updatehost'
	option cycle '15'
	option first '25'
	option path '/usr/share/sysmonitor/sysapp.sh'
	option name '更新主机'

config prog_list
	option enabled '1'
	option program 'chkvpn'
	option cycle '65'
	option first '120'
	option path '/usr/share/sysmonitor/sysapp.sh'
	option name '测连VPN'

config prog_list
	option enabled '1'
	option program 'reg_ddns'
	option cycle '3600'
	option path '/usr/share/sysmonitor/sysapp.sh'
	option first '60'
	option name '注册DDNS'
	
config prog_list
	option enabled '1'
	option program 'getddnsip'
	option cycle '900'
	option path '/usr/share/sysmonitor/sysapp.sh'
	option first '90'
	option name '获取DDNSip'
